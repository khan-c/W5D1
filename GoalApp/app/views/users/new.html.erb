<h1>New User</h1>

<form class="" action="<%= users_url %>" method="post">

  <label>Username
    <input type="text" name="user[username]" value="<%= @user.username %>">
  </label>

  <label>Password
    <input type="password" name="user[password]">
  </label>

  <input type="submit" name="" value="Sign Up">
</form>
